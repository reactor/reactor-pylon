{
  "name": "reactor-pylon",
  "version": "0.1.0",
  "description": "The Reactive Monitoring Experience for Reactor",
  "main": "",
  "scripts": {
    "clean": "rimraf src/main/resources/public/*",
    "copy": "copyfiles -f ./src/main/app/index.html ./src/main/resources/public",
    "copy:images" : "copyfiles -u 4 ./src/main/app/assets/images/* ./src/main/app/assets/images/**/* ./src/main/resources/public/assets",
    "copy:all" : "npm run copy & copyfiles -u 4 ./src/main/app/assets/images/* ./src/main/app/assets/images/**/* ./src/main/resources/public/assets",
    "copy:build:images" : "copyfiles -u 4 ./src/main/resources/public/assets/* ./src/main/resources/public/assets/**/**/* ./build/classes/main/public",
    "copy:build": "npm run copy:all & npm run copy:build:images & copyfiles -f ./src/main/app/index.html ./build/classes/main/public",
    "dist": "npm run copy:build & webpack --env=dist",
    "lint": "eslint ./src/main/app",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": "",
  "keywords": [],
  "author": "Stephane Maldini <smaldini@pivotal.io>",
  "contributors": [
    {
      "name": "Damien vitrac",
      "email": "damien@vitrac.me"
    },
    {
      "name": "Stephane Maldini",
      "email": "smaldini@pivotal.io"
    }
  ],
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "bower-webpack-plugin": "^0.1.9",
    "estraverse-fb": "^1.3.1",
    "chai": "^3.2.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.0",
    "file-loader": "^0.8.4",
    "glob": "^7.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.0.0",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.9",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0",
    "sass-loader": "^3.1.2",
    "node-sass": "^3.4.2",
    "compass-mixins": "^0.12.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "raw-loader": "^0.5.1"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-beta.2",
    "core-js": "^2.0.0",
    "d3": "^3.5.16",
    "font-awesome": "^4.4.0",
    "gridle": "^2.0.44",
    "moment": "^2.12.0",
    "normalize.css": "^3.0.3",
    "nvd3": "^1.8.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^2.0.0",
    "reactor-core-js": "^0.3.0",
    "classnames": "^2.2.5"
  }
}
